    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.13.0",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
