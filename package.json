    "consola": "^2.15.0",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "helmet": "^4.6.0",
    "k-domains": "^1.2.5",
    "module-alias": "^2.2.2",
    "nocache": "^3.0.1",
    "nuxt": "^2.15.3",
    "sass": "^1.35.2",
    "sass-loader": "^10",
    "vue-template-compiler": "^2.6.14"
